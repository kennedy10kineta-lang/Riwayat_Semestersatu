#include <iostream>
using namespace std;

int main()
{
    int suhu;
    
    cout << "Masukan angka : ";
    cin >> suhu;
    
    if (suhu < 20) {
        cout << "Pakai Jaket" << endl;
    } else if (suhu == 20) {
        cout << "Boleh pakai jaket, boleh juga tidak" << endl;
    } else {
        cout << "Tidak pakai jaket" << endl;
    }

    return 0;
}
